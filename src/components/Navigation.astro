---
import SetmoreButton from "./SetmoreButton.astro";
interface Props {
	id?: string;
	background?: string;
	boxed?: boolean;
}

const { id, background, boxed } = Astro.props;
---

<div class="nav">
	<h1 class="home">Spanish With Liz</h1>
	<SetmoreButton />
</div>
<style>
	.nav {
		display: flex;
		justify-content: space-between;
		align-items: center;
		width: 100%;
		height: 73px;
		background: #a15987;
	}
	h1 {
		padding: 0 2em;
		color: white;
		font-size: 36px;
		font-family: Concert One;
		font-weight: 400;
		word-wrap: break-word;
	}
</style>
